generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model VoicemailOrder {
  id            String   @id @default(cuid())
  email         String
  displayName   String
  script        String
  audioUrl      String?
  paymentMethod String
  paid          Boolean  @default(false)
  createdAt     DateTime @default(now())
}
